{
  "name": "brk/impressum",
  "description": "WordPress MU Plugin für automatische Impressum-Generierung für BRK-Unterseiten",
  "type": "wordpress-muplugin",
  "license": "GPL-2.0-or-later",
  "version": "1.0.0",
  "authors": [
    {
      "name": "BRK",
      "homepage": "https://brk.de"
    }
  ],
  "support": {
    "issues": "https://github.com/weidnerd/BRK-Impressum/issues",
    "source": "https://github.com/weidnerd/BRK-Impressum"
  },
  "require": {
    "php": ">=7.4",
    "composer/installers": "^1.0 || ^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.5",
    "wp-coding-standards/wpcs": "^2.3"
  },
  "autoload": {
    "classmap": [
      "includes/",
      "admin/"
    ]
  },
  "scripts": {
    "phpcs": "phpcs --standard=WordPress includes/ admin/ brk-impressum.php",
    "phpcbf": "phpcbf --standard=WordPress includes/ admin/ brk-impressum.php",
    "test": "phpunit"
  },
  "config": {
    "vendor-dir": "vendor",
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
